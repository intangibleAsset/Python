<script>
$(document).ready(function() {
$('.u-img-crop').each(
    function() {
        var i = $(this).find('img');
        if(i[0].clientHeight >= i[0].clientWidth) {
            $(this).addClass('u-img-crop__tall');
        }
    }
);
});
</script>





